public class Example 
{ 
public static void main(String[] args) 
{ 
int n, maxi; 
Scanner in = new Scanner(System.in);  
n = in.nextInt(); 
int j[] = new int[n];  
for(int i = 0; i < n; i++) 
{ 
a[i] = in.nextInt(); 
} 
maxi = j[0]; 
for(int i = 0; i < n; i++) 
{ 
if(maxi < j[i]) 
{ 
maxi = j[i]; 
} 
} 
